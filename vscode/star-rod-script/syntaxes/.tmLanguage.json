{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.",
				"match": "\\b(Add|Bind|BindLock|Call|End|Exec|ExecWait|SetPriority|SetTimescale|SetGroup|Unbind|Kill|Jump|SuspendAll|ResumeAll|SuspendOthers|ResumeOthers|Suspend|Set|Sub|Resume DoesScriptExist|Wait|WaitSeconds|Jump|Return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": ""
}