{
    "name": "star-rod-script",
    "publisher": "sonicspiral",
    "displayName": "Star Rod Script",
    "description": "Syntax colors for Paper Mario 64 ROM patching tool",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "star-rod-script",
            "aliases": ["Star Rod Script", "strod"],
            "extensions": [".bpat, .bscr, .mpat, .mscr"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "strod",
            "scopeName": "source.strod",
            "path": "./syntaxes/.tmLanguage.json"
        }]
    },
    "repository": {
		"type": "git",
		"url": "https://github.com/gregdegruy/star-rod.git"
	}
}